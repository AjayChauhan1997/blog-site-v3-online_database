<%- include("header.ejs") -%>
<div class="green text-center"><h3><%= msgTrue %></h3></div>


<div class="loginform text-center">
    
    
        <div class="form-signin w-100 m-auto">
          <form action="/login" method="post" >
            <h1 class="h3 mb-3 fw-normal"> <i class="fa-solid fa-lock fa-2x"></i></h1>
            <h1 class="h3 mb-3 fw-normal">Welcome Admin!</h1>
            <h1 class="h3 mb-3 fw-normal ">Please sign in</h1>
        
            <div class="form-floating ">
              <input type="email" class="form-control" id="floatingInput" name="id" placeholder="name@example.com">
              
            </div>
            <div class="form-floating">
              <input type="password" class="form-control" id="floatingPassword" name="pass" placeholder="Password">
              
            </div>
        
            <h5><%= msg %></h5>
            <button class="w-100 btn btn-lg btn-primary" type="submit">Sign in</button>
            
          </form>
        </div>
        
        
            
          
</div>


    
<%- include("footer.ejs") -%>
  
   
